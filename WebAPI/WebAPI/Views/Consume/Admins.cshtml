@model IEnumerable<WebAPI.Models.App_Admin>

@{
    ViewBag.Title = "Admins";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br />
<p>
    @Html.ActionLink("Create New", "SendData")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Admin_FullName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Admin_Contact)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Admin_Gender)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Admin_Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Admin_IsActive)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Admin_DOB)
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Admin_FullName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Admin_Contact)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Admin_Gender)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Admin_Email)
            </td>   
            <td>
                @Html.DisplayFor(modelItem => item.Admin_IsActive)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Admin_DOB)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Admin_ID }) |
                @Html.ActionLink("Details", "Details", new { id = item.Admin_ID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Admin_ID })
            </td>
        </tr>
    }
</table>
